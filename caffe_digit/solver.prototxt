net: "lenet_mnist_train.prototxt"
test_iter: 127
test_interval: 500
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "inv"
gamma: 0.0001
power: 0.75
# Display every 100 iterations
display: 100
# The maximum number of iterations
max_iter: 10000
# solver mode: CPU or GPU
solver_mode: CPU

